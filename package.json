{
  "name": "react-lazy-mount",
  "version": "1.1.0",
  "description": "Lazy mount react components once a certain condition is met.",
  "author": "BenBrewerBowman",
  "license": "MIT",
  "repository": "BenBrewerBowman/react-lazy-mount",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest -i",
    "build": "rollup -c; cp ./package.json ./dist/package.json; cp ./README.md ./dist/README.md; cp ./LICENSE ./dist/LICENSE",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.5.5",
    "@testing-library/react": "^9.2.0",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.9.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gh-pages": "^2.0.1",
    "react": "^16.10.1",
    "react-dom": "16.10.1",
    "react-scripts": "^3.1.2",
    "react-testing-library": "^8.0.1",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.2.4"
  }
